@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsEstacionesModel
@{
    var estacion = Model.Estacion;
}
 
<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id: </b> @estacion.id</li>
  <li class="list-group-item"> <b>Marca: </b> @estacion.nombre</li>
  <li class="list-group-item"> <b>Modelo: </b> @estacion.direccion</li>
  <li class="list-group-item"><b>Kilometraje: </b> @estacion.coord_x</li>
  <li class="list-group-item"><b>Numero de asientos: </b> @estacion.coord_y</li>
  <li class="list-group-item"><b>Placa: </b> @estacion.Tipo</li>
</ul>	